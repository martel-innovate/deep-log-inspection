                "msg_request_ip_local" : { "type" : "ip" },
                "msg_method" : { "type" : "keyword" },
                "msg_url" : { "type" : "text" },
                "msg_http_ver" : { "type" : "keyword" },
                "msg_response_status" : { "type" : "keyword" },
                "msg_bytes" : { "type" : "short" },
                "msg_time_sec" : { "type" : "float" }
